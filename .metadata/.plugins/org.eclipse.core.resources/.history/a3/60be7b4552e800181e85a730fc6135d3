
public class Dog2 {
	
	public String name;
	public int age;
	public String breed;
	public String color;
	
	public Dog2() {
		name="Hunter";
		age=15;
		breed="Poodle";
		color="White";
	}
	public void bark() {
		System.out.println(name+" is barking!");
	}
	
	public void eat() {
		System.out.println(name+" is eating!");
	}
	
	public void wagTail() {
		System.out.println(name+" is wagging tail");
	}
}
